{
  "name": "react-hacker",
  "version": "3.0.0",
  "description": "A simple React starter that's perfect for messing around. Comes with Babel, Emotion CSS in JS, ESlint, and Prettier.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --open",
    "build": "webpack --config webpack.config.js",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "postinstall": "npm run build",
    "fireStoreInit": "node --experimental-json-modules ./src/scripts/initDBData.js"
  },
  "author": "Daniel Zuzevich",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "babel-plugin-styled-components": "^1.10.1",
    "bootstrap": "^4.5.0",
    "check-node-version": "^4.0.3",
    "firebase": "^7.24.0",
    "react": "^16.12.0",
    "react-bootstrap": "^1.2.1",
    "react-dom": "^16.12.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "unique-hash": "^2.6.0"
  },
  "devDependencies": {
    "@babel/compat-data": "7.9.0",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "^5.0.2",
    "prettier": "^1.18.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  }
}
